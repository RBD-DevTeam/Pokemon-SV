static void HandleRedrawTradeMenuOnSide(u8 side)
{
    s8 nameStringWidth;
    u8 nickname[20];
    u8 movesString[56];
    u8 i;
    u8 partyIdx;
    u8 whichParty;
    u8 monIdx = sTradeMenuResourcesPtr->menuRedrawCursorPos[side];

    whichParty = 1;
    if (sTradeMenuResourcesPtr->menuRedrawCursorPos[side] < 6)
        whichParty = 0;
    partyIdx = monIdx % 6;
    nameStringWidth = 0;

    switch (sTradeMenuResourcesPtr->menuRedrawState[side])
    {
    case 1:
        for (i = 0; i < sTradeMenuResourcesPtr->partyCounts[side]; i++)
        {
            gSprites[sTradeMenuResourcesPtr->partyIcons[0][i + (whichParty * 6)]].invisible = 1;
        }

        for (i = 0; i < 6; i++)
        {
            ClearWindowTilemap(i + (side * 6 + 2));
        }

        gSprites[sTradeMenuResourcesPtr->partyIcons[0][partyIdx + (whichParty * 6)]].invisible = 0;
        gSprites[sTradeMenuResourcesPtr->partyIcons[0][partyIdx + (whichParty * 6)]].data[0] = 20;
